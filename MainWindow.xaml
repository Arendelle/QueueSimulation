                                <ListBoxItem Content="顾客平均逗留时间Ws"/>
                                <ListBoxItem Content="顾客平均等待时间Wq"/>
                                <ListBoxItem Content="忙期平均等待时间Wb"/>
                                <ListBoxItem Content="平均队长Ls"/>
                                <ListBoxItem Content="平均等待队长Lq"/>
                            </ComboBox>
                            <Label Content="评价指标：" HorizontalAlignment="Left" Margin="677,28,0,0" VerticalAlignment="Top" FontSize="18" RenderTransformOrigin="0.5,0.5"/>
